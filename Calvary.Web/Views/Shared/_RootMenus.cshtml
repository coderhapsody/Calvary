@model IEnumerable<Calvary.ViewModels.Security.MenuAccess.ListMenuViewModel>

@foreach (var menu in Model)
{
    <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"> @menu.Name <span class="caret"></span></a>
        @Html.Action("GetMenus", "Home", new { parentMenuId = menu.id ,area = String.Empty})
    </li>

}
